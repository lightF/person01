package sec02.exam07;

public class ForfloatCounterExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//for (초기값; 최종값; 증가값) {
			//var float x = 0.f
			// A = A + B ,짧게 A += B
		//}
		for(float x=0.1f; x<-1.0f; x+=0.1f) {
			System.out.println(x);
		}

	}

}
